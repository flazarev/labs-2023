#include <stdio.h>
#define UINT_MAX 0xFFFFFFFFF

int main() {
    int num = 0;
    while (1) {
        int digit = getchar();
        if ((digit != EOF) && (digit != '\n')) {
        num = (num * 10 + (digit - '0'));
        } else {
            break;
        }
    }
    int reverse_num = UINT_MAX - num;
    printf("%lld", reverse_num);
    return 0;
}
